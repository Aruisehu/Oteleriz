<section class="-border-bottom">
    <a id="menu" class="anchor"></a>
    <div class="row">
        <h2 class="title align -center"><%= raw(I18n.t("home.menu.title")) %></h2>
        <h3 class="subtitle align -center"><%= raw(I18n.t("home.menu.subtitle")) %></h3>
        <div class="faded-zone">
            <%= cell(:product, collection: @starters, presentation: true).(:tiny) %>
        </div>
        <div class="align -center">
            <a class="btn" href="<%= orders_path %>"><%= raw(I18n.t("buttons.menu")) %><%= fa_icon "book" %></a>
            <a class="btn -alt" href="<%= orders_path %>#course"><%= raw(I18n.t("buttons.formula")) %><%= fa_icon "edit" %></a>
