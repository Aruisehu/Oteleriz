<% content_for :main_action do %>
    <%= link_to orders_path, class: "btn" do %>
        <%= I18n.t("page.menu") %> <%= fa_icon "book" %>
    <% end %>
<% end %>
<section class="head" id="home">
    <div class="row">
        <%= image_tag "logo-white.png" %>
        <h5 class="title"><%= I18n.t("slogan") %></h5>
        <a class="btn -white" href="<%= orders_path %>"><%= I18n.t("page.menu") %> <%= fa_icon "book" %></a>
        <a class="btn -white -alt" href="#concept"><%= I18n.t("buttons.more_full") %> <%= fa_icon "info" %></a>
        <hr>
        <a class="btn -white" href="<%= order_access_path %>"><%= I18n.t("app_name") %> *<%= fa_icon "shopping-basket" %></a>
        <p>*<%= I18n.t("buttons.tooltip") %></p>
    </div>
</section>
